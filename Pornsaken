-- âœ… Forsaken Rayfield GUI (Delta Executor Compatible + Rayfield UI) -- ANGEL HUB | Features: Auto Block (Guest 1337), Infinite Stamina, Auto Punch, Generator Solve, ESPs

-- Load Rayfield local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({ Name = "ANGEL HUB", LoadingTitle = "ANGEL HUB", LoadingSubtitle = "by ChatGPT", ConfigurationSaving = { Enabled = true, FolderName = nil, FileName = "AngelHubCfg" }, Discord = { Enabled = false }, KeySystem = false })

local Player = game.Players.LocalPlayer local Character = Player.Character or Player.CharacterAdded:Wait() local RunService = game:GetService("RunService")

-- Feature toggles and settings local AutoBlockEnabled = false local AutoBlockRange = 15 local AutoPunchEnabled = false local AutoPunchRange = 15 local InfiniteStaminaEnabled = false local SolveGenEnabled = false local KillerESPEnabled = false local SurvivorESPEnabled = false local GenESPEnabled = false local ItemESPEnabled = false

local blockTools = {"slash","punch","gashing wound","behead","walkspeed override","stab","void rush"} local killers = {"Jason","Noli","John Doe","1x1x1x1","coolkid"} local survivors = {"Guest 1337","007n7","Two Time","Chance","Builderman","Noob","Elliot","Dusekkar"}

function highlight(obj, color) if not obj:FindFirstChild("Highlight") then local h = Instance.new("Highlight") h.FillColor = color h.OutlineColor = Color3.new(1,1,1) h.FillTransparency = 0.3 h.OutlineTransparency = 0.5 h.Adornee = obj h.Parent = obj task.delay(5, function() h:Destroy() end) end end

-- Auto Block Window:CreateSlider({ Name = "Auto Block Range", Range = {0, 25}, Increment = 1, CurrentValue = 15, Callback = function(v) AutoBlockRange = v end })

Window:CreateToggle({ Name = "Auto Block (Guest 1337)", CurrentValue = false, Callback = function(v) AutoBlockEnabled = v task.spawn(function() while AutoBlockEnabled and task.wait(0.0001) do if Player.Name ~= "Guest 1337" then continue end local tool = Character:FindFirstChildOfClass("Tool") if tool and tool:FindFirstChild("RemoteEvent") then for _, p in pairs(game.Players:GetPlayers()) do if p ~= Player and p.Character and p:DistanceFromCharacter(Character.HumanoidRootPart.Position) <= AutoBlockRange then local ttool = p.Character:FindFirstChildOfClass("Tool") if ttool and table.find(blockTools, ttool.Name:lower()) then tool.RemoteEvent:FireServer() end end end end end end) end })

-- Auto Punch Window:CreateSlider({ Name = "Auto Punch Range", Range = {0, 25}, Increment = 1, CurrentValue = 15, Callback = function(v) AutoPunchRange = v end })

Window:CreateToggle({ Name = "Auto Punch", CurrentValue = false, Callback = function(v) AutoPunchEnabled = v task.spawn(function() while AutoPunchEnabled and task.wait(0.0001) do local tool = Character:FindFirstChildOfClass("Tool") if tool then for _, p in pairs(game.Players:GetPlayers()) do if p ~= Player and p.Character and p:DistanceFromCharacter(Character.HumanoidRootPart.Position) <= AutoPunchRange then tool:Activate() end end end end end) end })

-- Infinite Stamina Window:CreateToggle({ Name = "Infinite Stamina", CurrentValue = false, Callback = function(v) InfiniteStaminaEnabled = v task.spawn(function() while InfiniteStaminaEnabled and task.wait(0.1) do local s = Character and Character:FindFirstChild("Stamina") if s then s.Value = s.MaxValue or 100 end end end) end })

-- Auto Generator Window:CreateToggle({ Name = "Auto Solve Generator", CurrentValue = false, Callback = function(v) SolveGenEnabled = v task.spawn(function() while SolveGenEnabled and task.wait(1.5) do for _, g in pairs(workspace:GetDescendants()) do if g.Name:lower():find("puzzle") and g:FindFirstChild("RemoteEvent") then g.RemoteEvent:FireServer() end end end end) end })

-- ESP Toggles Window:CreateToggle({ Name = "Killer ESP", CurrentValue = false, Callback = function(v) KillerESPEnabled = v task.spawn(function() while KillerESPEnabled and task.wait(0.5) do for _, p in pairs(game.Players:GetPlayers()) do if p ~= Player and p.Character and table.find(killers, p.Name) then highlight(p.Character, Color3.new(1,0,0)) end end end end) end })

Window:CreateToggle({ Name = "Survivor ESP", CurrentValue = false, Callback = function(v) SurvivorESPEnabled = v task.spawn(function() while SurvivorESPEnabled and task.wait(0.5) do for _, p in pairs(game.Players:GetPlayers()) do if p ~= Player and p.Character and table.find(survivors, p.Name) then highlight(p.Character, Color3.new(0,1,0)) end end end end) end })

Window:CreateToggle({ Name = "Generator ESP", CurrentValue = false, Callback = function(v) GenESPEnabled = v task.spawn(function() while GenESPEnabled and task.wait(1) do for _, obj in pairs(workspace:GetDescendants()) do if obj:IsA("BasePart") and obj.Name:lower():find("generator") then highlight(obj, Color3.new(1,1,0)) end end end end) end })

Window:CreateToggle({ Name = "Item ESP (Cola/Medkit)", CurrentValue = false, Callback = function(v) ItemESPEnabled = v task.spawn(function() while ItemESPEnabled and task.wait(1) do for _, obj in pairs(workspace:GetDescendants()) do if obj:IsA("BasePart") and (obj.Name:lower():find("cola") or obj.Name:lower():find("medkit")) then highlight(obj, Color3.new(0.5,0.5,1)) end end end end) end })

    Flag = "ItemESP",
    Callback = function(value)
        espEnabled.item = value
    end
})

Window:CreateToggle({
    Name = "Survivor ESP",
    CurrentValue = false,
    Flag = "SurvivorESP",
    Callback = function(value)
        espEnabled.survivor = value
    end
})

-- Main loops

-- Auto block & punch logic
RunService.Heartbeat:Connect(function()
    local target = getPlayerByName("Guest1337")
    if not target or not target.Character or not target.Character:FindFirstChild("HumanoidRootPart") then return end

    if autoBlockEnabled then
        -- Example block action: you need the game's block function, this is a placeholder
        -- e.g., fire a remote event or invoke a server method to block
        -- game:GetService("ReplicatedStorage").Events.Block:FireServer()
        -- Here just printing as example:
        print("Blocking against Guest1337")
    end

    if autoPunchEnabled then
        -- Example punch action: replace with actual punch remote event call
        -- game:GetService("ReplicatedStorage").Events.Punch:FireServer(target.Character.HumanoidRootPart)
        print("Punching Guest1337")
    end

    if punchFlingEnabled then
        -- Punch fling usually involves manipulating velocity or position to fling the target
        local hrp = target.Character:FindFirstChild("HumanoidRootPart")
        local localHRP = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
        if hrp and localHRP then
            -- This is a simple fling attempt, adjust magnitude & method per game
            local bodyVelocity = Instance.new("BodyVelocity")
            bodyVelocity.Velocity = (hrp.Position - localHRP.Position).unit * 100
            bodyVelocity.MaxForce = Vector3.new(1e5, 1e5, 1e5)
            bodyVelocity.Parent = hrp
            game.Debris:AddItem(bodyVelocity, 0.2)
            print("Punch fling applied on Guest1337")
        end
    end
end)

-- Infinite stamina logic (generic example)
RunService.Heartbeat:Connect(function()
    if infiniteStaminaEnabled and LocalPlayer.Character then
        local stamina = LocalPlayer.Character:FindFirstChild("Stamina")
        if stamina and stamina:IsA("NumberValue") then
            stamina.Value = stamina.MaxValue or 100
        end
    end
end)

-- ESP logic (generic; you must adjust paths to actual game objects)
RunService.Heartbeat:Connect(function()
    if espEnabled.killer then
        for _, plr in pairs(Players:GetPlayers()) do
            if plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
                -- Assuming killer has a BoolValue named "IsKiller"
                if plr:FindFirstChild("IsKiller") and plr.IsKiller.Value == true then
                    espForObject(plr.Character.HumanoidRootPart, Color3.new(1, 0, 0)) -- red
                end
            end
        end
    end

    if espEnabled.survivor then
        for _, plr in pairs(Players:GetPlayers()) do
            if plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
                -- Assuming survivors are players without IsKiller true
                if not (plr:FindFirstChild("IsKiller") and plr.IsKiller.Value == true) then
                    espForObject(plr.Character.HumanoidRootPart, Color3.new(0, 1, 0)) -- green
                end
            end
        end
    end

    if espEnabled.generator then
        -- Find generators in workspace (adjust path to actual generators)
        for _, gen in pairs(workspace:GetChildren()) do
            if gen.Name == "Generator" and gen:IsA("BasePart") then
                espForObject(gen, Color3.new(1, 1, 0)) -- yellow
            end
        end
    end

    if espEnabled.item then
        -- Find items in workspace (adjust path)
        for _, item in pairs(workspace:GetChildren()) do
            if item.Name == "Item" and item:IsA("BasePart") then
                espForObject(item, Color3.new(0, 0, 1)) -- blue
            end
        end
    end
end)

print("Forsaken Helper loaded! Use the GUI to toggle features.")
